{
  "name": "lite",
  "version": "1.0.0",
  "description": "",
  "main": "litegraph.min.js",
  "scripts": {
    "build": "node scripts/clean.js && node scripts/bundle.js && npm run bundle && npm run minify-css && npm run replace-scripts web && npm run minify-html  && npm run compress",
    "build-esp": "node scripts/clean.js && node scripts/bundle.js && npm run bundle && npm run minify-css && npm run replace-scripts esp && npm run minify-html  && npm run compress",
    "bundle": "npx uglify-js dist/bundle.min.js -o dist/bundle.min.js -c -m",
    "replace-scripts": "node scripts/replace-scripts.js",
    "minify-html": "html-minifier-terser --collapse-whitespace --remove-comments --minify-css true --minify-js true dist/index.html -o dist/index.html",
    "minify-css": "cleancss -o dist/style.min.css src/*.css",
    "compress": "brotli -q 11 dist/*.{html,css,js} && gzip -k dist/*.{html,css,js}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "brotli": "^1.3.3",
    "brotli-cli": "^2.1.1",
    "concat": "^1.0.3",
    "concat-cli": "^4.0.0",
    "esbuild": "^0.25.9",
    "html-minifier-terser": "^7.2.0",
    "uglify-js": "^3.19.3"
  },
  "dependencies": {
    "clean-css-cli": "^5.6.3",
    "glob": "^11.0.3"
  }
}
